--                                                         QUERY's

-- DROP TABLE TELEFONE;
-- DELETE FROM ENDERECO
-- WHERE ID_ENDERECO = 5;

SELECT C.ID_CLIENTE, C.NOME, C.EMAIL,  F.ID_CPF, F.NASCIMENTO, S.DESCRICAO
FROM
CLIENTE C
JOIN
FISICA F
ON
C.ID_CLIENTE  = F.CLIENTE_ID_CLIENTE
JOIN
SEXO S
ON
S.ID_SEXO = F.SEXO_ID_SEXO;

SELECT C.ID_CLIENTE, C.NOME, C.EMAIL, J.ID_CNPJ, J.ABERTURA, J.INSCRICAO_ESTADUAL, J.RAZAO_SOCIAL
FROM
CLIENTE C
JOIN
JURIDICA J
ON
J.CLIENTE_ID_CLIENTE = C.ID_CLIENTE;

SELECT J.RAZAO_SOCIAL,  J.ID_CNPJ , J.INSCRICAO_ESTADUAL,T.DDD, T.CELULAR , E.TIPO_LOGRADOURO, E.NOME_LOGRADOURO, E.NUMERO, E.CEP, E.CIDADE, ES.SIGLA  
FROM
CLIENTE C
INNER JOIN
JURIDICA J  
ON
J.CLIENTE_ID_CLIENTE  = C.ID_CLIENTE
INNER JOIN
TELEFONE T
ON
T.CLIENTE_ID_CLIENTE = C.ID_CLIENTE 
INNER JOIN 
CLIENTE_ENDERECO CE
ON
CE.CLIENTE_ID_ENDERECO = C.ID_CLIENTE
INNER JOIN 
ENDERECO E
ON
CE.ENDERECO_ID_ENDERECO = E.ID_ENDERECO
INNER JOIN
ESTADO ES
ON
E.ESTADO_ID_ESTADO = ES.ID_ESTADO;

-- Query Detalhes pedido pessoa juridica
SELECT P.ID_PEDIDO, DATE_FORMAT(P.DATA_PEDIDO, '%d-%m-%Y') AS 'DATA' , P.VALOR_TOTAL, PD.DESCRICAO, PD.VOLUME, PD.PESO, PD.PRECO, IP.QUANTIDADE, 
SP.DESCRICAO, C.NOME, C.EMAIL, J.ID_CNPJ, J.RAZAO_SOCIAL, T.DDD, T.CELULAR, E.TIPO_LOGRADOURO, E.NOME_LOGRADOURO,
E.NUMERO, E.CEP, E.CIDADE, ES.SIGLA, DATE_FORMAT(PG.DATA_PAGAMENTO, '%d-%m-%Y') AS 'DATA', PG.STATUS_PAGAMENTO, PG.VALOR_PAGAMENTO, FP.INSTITUICAO,
ENT.PRAZO_ESTIMADO, ENT.VALOR, TSE.DESCRICAO, DATE_FORMAT(SE.DATA_ENTREGA, '%d-%m-%Y') AS 'DATA'    
FROM
PEDIDO P
INNER JOIN
ITEM_PEDIDO IP
ON
IP.PEDIDO_ID_PEDIDO = P.ID_PEDIDO
INNER JOIN 
PRODUTO PD
ON 
IP.PRODUTO_ID_PRODUTO = PD.ID_PRODUTO
INNER JOIN
STATUS_PEDIDO SP
ON 
SP.PEDIDO_ID_PEDIDO = P.ID_PEDIDO
INNER JOIN
CLIENTE C
ON 
P.CLIENTE_ID_CLIENTE = C.ID_CLIENTE
INNER JOIN
JURIDICA J  
ON
J.CLIENTE_ID_CLIENTE  = C.ID_CLIENTE
INNER JOIN
TELEFONE T
ON
T.CLIENTE_ID_CLIENTE = C.ID_CLIENTE 
INNER JOIN 
CLIENTE_ENDERECO CE
ON
CE.CLIENTE_ID_ENDERECO = C.ID_CLIENTE
INNER JOIN 
ENDERECO E
ON
CE.ENDERECO_ID_ENDERECO = E.ID_ENDERECO
INNER JOIN
ESTADO ES
ON
E.ESTADO_ID_ESTADO = ES.ID_ESTADO
INNER JOIN
PAGAMENTO PG
ON
PG.PEDIDO_ID_PEDIDO = P.ID_PEDIDO
INNER JOIN
FORMA FP
ON
PG.FORMA_ID_FORMA = FP.ID_FORMA
INNER JOIN
ENTREGA ENT
ON
P.ENTREGA_ID_ENTREGA  = ENT.ID_ENTREGA
INNER JOIN
FORMA_ENTREGA FENT
ON 
ENT.FORMA_ENTREGA_ID_FORMA_ENTREGA = FENT.ID_FORMA_ENTREGA
INNER JOIN
STATUS_ENTREGA SE 
ON
SE.ENTREGA_ID_ENTREGA = ENT.ID_ENTREGA
INNER JOIN
TIPO_STATUS_ENTREGA TSE
ON
TSE.STATUS_ENTREGA_ID_STATUS_ENTREGA = SE.ID_STATUS_ENTREGA; 


-- Query Detalhes pedido pessoa fisica
SELECT P.ID_PEDIDO, DATE_FORMAT(P.DATA_PEDIDO,  '%d-%m-%Y') AS 'DATA', P.VALOR_TOTAL, PD.DESCRICAO, PD.VOLUME, PD.PESO, PD.PRECO, IP.QUANTIDADE, 
SP.DESCRICAO, C.NOME, C.EMAIL, F.ID_CPF , T.DDD, T.CELULAR, E.TIPO_LOGRADOURO, E.NOME_LOGRADOURO,
E.NUMERO, E.CEP, E.CIDADE, ES.SIGLA, DATE_FORMAT(PG.DATA_PAGAMENTO, '%d-%m-%Y') AS 'DATA', PG.STATUS_PAGAMENTO, PG.VALOR_PAGAMENTO, FP.INSTITUICAO,
ENT.PRAZO_ESTIMADO, ENT.VALOR, TSE.DESCRICAO, DATE_FORMAT(SE.DATA_ENTREGA, '%d-%m-%Y') 
FROM
PEDIDO P
INNER JOIN
ITEM_PEDIDO IP
ON
IP.PEDIDO_ID_PEDIDO = P.ID_PEDIDO
INNER JOIN 
PRODUTO PD
ON 
IP.PRODUTO_ID_PRODUTO = PD.ID_PRODUTO
INNER JOIN
STATUS_PEDIDO SP
ON 
SP.PEDIDO_ID_PEDIDO = P.ID_PEDIDO
INNER JOIN
CLIENTE C
ON 
P.CLIENTE_ID_CLIENTE = C.ID_CLIENTE
INNER JOIN
FISICA F 
ON
F.CLIENTE_ID_CLIENTE  = C.ID_CLIENTE
INNER JOIN
TELEFONE T
ON
T.CLIENTE_ID_CLIENTE = C.ID_CLIENTE
INNER JOIN 
CLIENTE_ENDERECO CE
ON
CE.CLIENTE_ID_ENDERECO = C.ID_CLIENTE
INNER JOIN 
ENDERECO E
ON
CE.ENDERECO_ID_ENDERECO = E.ID_ENDERECO
INNER JOIN
ESTADO ES
ON
E.ESTADO_ID_ESTADO = ES.ID_ESTADO
INNER JOIN
PAGAMENTO PG
ON
PG.PEDIDO_ID_PEDIDO = P.ID_PEDIDO
INNER JOIN
FORMA FP
ON
PG.FORMA_ID_FORMA = FP.ID_FORMA
INNER JOIN
ENTREGA ENT
ON
P.ENTREGA_ID_ENTREGA  = ENT.ID_ENTREGA
INNER JOIN
FORMA_ENTREGA FENT
ON 
ENT.FORMA_ENTREGA_ID_FORMA_ENTREGA = FENT.ID_FORMA_ENTREGA
INNER JOIN
STATUS_ENTREGA SE 
ON
SE.ENTREGA_ID_ENTREGA = ENT.ID_ENTREGA
INNER JOIN
TIPO_STATUS_ENTREGA TSE
ON
TSE.STATUS_ENTREGA_ID_STATUS_ENTREGA = SE.ID_STATUS_ENTREGA;





